<div class="container-center">
  <h1><%= question_header(@question) %></h1>
  <%= form_for [:admin, @test, @question] do |form| %>
    <p>
      <%= form.label :body %>
      <%= form.text_field :body %>
      <%= form.submit %>
    </p>
  <% end %>
  <%= link_to 'Back', admin_test_path(@question.test) %>
</div>
